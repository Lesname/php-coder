{
  "name": "lesname/coder",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",

    "ext-mbstring": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",

    "phpunit/phpunit": "^12.0.0",

    "phpstan/phpstan": "^2.0.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "LesCoder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesCoderTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
